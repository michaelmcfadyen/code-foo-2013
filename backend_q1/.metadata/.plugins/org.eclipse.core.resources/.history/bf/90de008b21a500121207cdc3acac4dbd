
public class FamilyTree {
	
	public static void main(String args[]){
		familyMember Dad = new familyMember("Dave","Jones");
		familyMember Mum = new familyMember("Susan","Jones");
		familyMember son = new familyMember(Mum,Dad,"Steve","Jones",SEX.MALE);
		System.out.println(son.generation());
		System.out.println(Dad.getChildren().toString());
	}

}
