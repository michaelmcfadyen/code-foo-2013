import java.util.ArrayList;


public class MergeSort {
	Float[] listToSort;
	Float[] storer;
	
	public void sort(ArrayList<Pair<Float,String>> scores){
		int len = scores.size();
		for(int i = 0; i < scores.size(); i++){
			listToSort[i] = scores.get(i).getScore();
		}
		storer = new Float[len];
		mergesort(0, len - 1);
	}
	
	public void mergesort(int low, int high){
		if(low < high){
			int middle = (low + high) / 2;
			mergesort(low,middle);
			mergesort(middle + 1, high);
			merge(low, middle, high);
		}
		

	}
	public void merge(int low, int middle, int high){
		for(int i = low; i < high; i++){
			storer[i] = listToSort[i];
		}
		int i = low;
		int j = middle + 1;
		while(i <= middle && j <= high){
			if(storer.get(i).getScore() <= storer.get(j).getScore()){
				
			}
		}
	}
	
	public static void main(String args[]){
		ArrayList<Pair<Float,String>> scores = new ArrayList<Pair<Float,String>>();
		
		
	}
}


